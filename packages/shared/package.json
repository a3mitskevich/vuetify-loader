{
  "name": "@vuetify/loader-shared",
  "version": "1.7.1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuetifyjs/vuetify-loader.git"
  },
  "scripts": {
    "generate-types": "tsx generate-ignore-types.ts",
    "build": "yarn generate-types && unbuild",
    "dev": "yarn generate-types && unbuild --stub"
  },
  "author": "Kael Watts-Deuchar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vuetifyjs/vuetify-loader/issues"
  },
  "homepage": "https://github.com/vuetifyjs/vuetify-loader/tree/master/packages/shared",
  "dependencies": {
    "find-cache-dir": "^5.0.0",
    "upath": "^2.0.1"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vuetify": "^3.0.0"
  },
  "devDependencies": {
    "tsx": "^4.6.2"
  },
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  }
}
